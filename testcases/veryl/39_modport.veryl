module Module39 (
    mst: modport Interface39::master,
    slv: modport Interface39::slave ,
) {
    var a    : logic;
    assign mst.a = a;
    assign a     = slv.get_a();

    inst module_another: ModuleAnother (
        mst  ,
        slv  ,
    );
}

interface Interface39 {
    var a: logic;

    function get_a () -> logic {
        return a;
    }

    modport master {
        a: output,
    }

    modport slave {
        a    : input ,
        get_a: import,
    }
}

module ModuleAnother (
    mst: modport Interface39::master,
    slv: modport Interface39::slave ,
) {}
