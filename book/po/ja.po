"UnionTerm             : /(?-u:\\b)union(?-u:"
"\\b)/                                                               : "
"Token;\n"
"VarTerm               : /(?-u:\\b)var(?-u:"
"\\b)/                                                                 : "
"Token;\n"
"IdentifierTerm        : /[a-zA-Z_][0-9a-zA-Z_"
"$]*/                                                             : Token;\n"
"\n"
